msgstr "Podsumowanie"

#. TRANSLATORS: We failed to find any component in the database due to an error
#, c-format
msgid "Unable to find component matching %s!"
msgstr "Nie można odnaleźć składnika pasującego do %s."

#, c-format
msgid "Unable to find component providing '%s:%s:%s'!"
msgstr "Nie można odnaleźć składnika dostarczającego \"%s:%s:%s\"."

#, c-format
msgid "Unable to find component with id '%s'!"
msgstr "Nie można odnaleźć składnika o identyfikatorze \"%s\"."

msgid "Validation failed."
msgstr "Sprawdzenie poprawności się nie powiodło."

#, c-format
msgid "XDG Menu XML file '%s' is damaged."
msgstr "Plik menu XDG w formacie XML \"%s\" jest uszkodzony."

msgid "You need to run this command with superuser permissions!"
msgstr "Należy wykonać to polecenie z uprawnieniami superużytkownika."

msgid "You need to specify a command."
msgstr "Należy podać polecenie."

msgid "You need to specify a component-id."
msgstr "Należy podać identyfikator składnika."

msgid "You need to specify a file to validate!"
msgstr "Należy podać plik do sprawdzenia poprawności."

msgid "You need to specify a term to search for."
msgstr "Należy podać termin do wyszukania."

