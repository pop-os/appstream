#, c-format
msgid "Unable to find component matching %s!"
msgstr "Nie je možné nájsť súčasť zodpovedajúcu %s!"

#, c-format
msgid "Unable to find component providing '%s;%s'!"
msgstr "Nie je možné nájsť komponent poskytujúci „%s;%s“!"

#, c-format
msgid "Unable to find component with id '%s'!"
msgstr "Nie je možné nájsť súčasť s ID „%s“!"

